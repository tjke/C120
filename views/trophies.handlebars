	<!-- main content of the page goes here -->
	<div class="main">
			<div class=" containter-fluid page-title bg-1 text-center">
				<h1>TROPHIES</h1>
			</div>
			<div class="media">
			  <a class="pull-left">
			    <img class="media-object" src="/images/trophy.png" alt="...">
			  </a>
			  <div class="media-body">
			    <h4 class="media-heading">Trophy A</h4>
			    <p>Complete 10 challenges</p>
			    <div class="progress progress-striped active">
			  <div class="progress-bar progress-bar-danger" role="progressbar" aria-valuenow="80" aria-valuemin="0" aria-valuemax="100" style="width: 80%">
			    8/10
			    <span class="sr-only">80% Complete (danger)</span>
			  </div>
			  </div>
			  </div>
			</div>

			<div class="media">
			  <a class="pull-left">
			    <img class="media-object" src="/images/trophy.png" alt="...">
			  </a>
			  <div class="media-body">
			    <h4 class="media-heading">Trophy B</h4>
			    <p>Donate to 10 organizations</p>
			    <div class="progress progress-striped active">
			  <div class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="40" aria-valuemin="0" aria-valuemax="100" style="width: 40%">
			    4/10
			  </div>
			  </div>
			  </div>
			</div>

			<div class="media">
			  <a class="pull-left">
			    <img class="media-object" src="/images/trophy.png" alt="...">
			  </a>
			  <div class="media-body">
			    <h4 class="media-heading">Trophy C</h4>
			    <p>Open the app for 10 days continuously</p>
			    <div class="progress progress-striped active">
			  <div class="progress-bar progress-bar-info" role="progressbar" aria-valuenow="20" aria-valuemin="0" aria-valuemax="100" style="width: 20%">
			    2/10
			  </div>
			  </div>
			  </div>
			</div>

			<div class="media">
			  <a class="pull-left">
			    <img class="media-object" src="/images/trophy.png" alt="...">
			  </a>
			  <div class="media-body">
			    <h4 class="media-heading">Trophy D</h4>
			    <p>Share challenges to 10 friends</p>
			    <div class="progress progress-striped active">
			  <div class="progress-bar progress-bar-warning" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width: 60%">
			    6/10
			  </div>
			  </div>
			  </div>
			</div>
